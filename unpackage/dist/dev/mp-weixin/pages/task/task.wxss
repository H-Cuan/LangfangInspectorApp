
page {
	width: 100%;
	background-color: #f2f5f7;
}
cell {
	display: flex;
	justify-content: center;
}
.nav {
	width: 100%;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: space-around;
	background-color: #fff;
	font-size: 30rpx;
	letter-spacing: 2.14rpx;
}
.nav-list {
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	width: 20%;
	height: 100%;
	margin-top: 40rpx;
	opacity: .9;
}
.nav-list .name {
	color: #5085FF;
}
.nav-list .line {
	margin-top: 10rpx;
	width: 65%;
	height: 4rpx;
	background-color: #5085FF;
}
.list {
	padding-top: 10px;
	display: flex;
	width: 90%;
	border-radius: 10px;
	justify-content: space-around;
	margin: 10px 0 10px 0;
	background-color: #fff;
}

